{
  "name": "api.coffee.com",
  "version": "0.1.0",
  "description": "This is a simple Coffee API",
  "main": "index.js",
  "scripts": {
    "build": "swc src -d dist --copy-files --strip-leading-paths --delete-dir-on-start --ignore */.{spec,test}.ts && tsc && tsc-alias --project tsconfig.build.json",
    "dev": "tsx --env-file .env --watch src/app.ts",
    "lint": "eslint .",
    "prepare": "husky && husky install",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests"
  },
  "engines": {
    "node": "20.12.2"
  },
  "repository": {
    "url": "https://github.com/matheusscode/api.coffee.com"
  },
  "keywords": [
    "coffee"
  ],
  "homepage": "https://github.com/matheusscode/api.coffee.com",
  "author": "matheusscode",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "@jest/globals": "^29.7.0",
    "@swc/cli": "^0.5.2",
    "@swc/core": "^1.10.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.10.2",
    "eslint": "^9.16.0",
    "globals": "^15.13.0",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prisma": "^6.0.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.0"
  },
  "dependencies": {
    "@prisma/client": "^6.0.1",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "morgan": "^1.10.0"
  }
}
